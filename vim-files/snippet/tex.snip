snippet tab2
alias tab2
    \begin{table}[${1}]
        \begin{minipage}[t]{.45\textwidth}
        \centering
        \caption{${2}}
        \label{${3}}
        \begin{tabular}{${4}}
            ${5}
        \end{tabular}
        \end{minipage}
        \hfill
        \begin{minipage}[t]{.45\textwidth}
        \centering
        \caption{${6}}
        \label{${7}}
        \begin{tabular}{${8}}
            ${9}
        \end{tabular}
        \end{minipage}
    \end{table}

