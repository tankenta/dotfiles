snippet fig2
alias   fig2
    \begin{figure}[${1}]
        \begin{minipage}[b]{0.45\linewidth}
            \centering
            \includegraphics[width=70mm]{${2}}
            \caption{${3}}\label{${4}}
        \end{minipage}
        \begin{minipage}[b]{0.45\linewidth}
            \centering
            \includegraphics[width=70mm]{${5}}
            \caption{${6}}\label{${7}}
        \end{minipage}
    \end{figure}

snippet fig2sub
alias   fig2sub
    \begin{figure}[${1}]
        \begin{minipage}[b]{0.45\linewidth}
            \centering
            \includegraphics[width=70mm]{${2}}
            \subcaption{${3}}\label{${4}}
        \end{minipage}
        \begin{minipage}[b]{0.45\linewidth}
            \centering
            \includegraphics[width=70mm]{${5}}
            \subcaption{${6}}\label{${7}}
        \end{minipage}
        \caption{${8}}\label{${9}}
    \end{figure}

